@article{姜萍2008创新教育在,
  title={创新教育在EDA实验教学中的研究与实践},
  author={姜萍 and 蒋立平 and 谭雪琴 and 薛文},
  journal={世界华商经济年鉴·高校教育研究},
  year={2008},
 keywords={教学改革;网络教学;创新教育;分层次教学},
 abstract={EDA实验是数字逻辑电路理论教学的重要实践环节,其教学目标是使学生掌握现代化系统仿真分析与ASIC设计手段.通过实践和探索,本文从更新教学内容、 改进教学方法、革新教学模式等方面总结了经验,为进一步深化教学改革、提高教学质量作了有益的探讨和研究.},
}

